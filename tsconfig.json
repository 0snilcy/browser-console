{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",
		"lib": ["ES2019", "DOM"],
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"rootDir": "src",
		"esModuleInterop": true,
		"strictPropertyInitialization": false,
		"isolatedModules": true
	},
	"exclude": ["node_modules", ".vscode-test", "out"]
}
